  "homepage": "https://github.com/ng2workspace/ng2workspace-sample-app#readme",
  "author": "Merott Movahedi <merott@merott.com>",
  "license": "MIT",
  "dependencies": {
    "angular2": "2.0.0-beta.7",
    "es6-promise": "3.1.2",
    "es6-shim": "0.34.4",
    "es7-reflect-metadata": "1.5.5",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.5.15"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "gulp": "^3.9.1",
    "json-loader": "^0.5.4",
    "ng2workspace": "../ng2workspace",
    "ng2workspace-addon-webpack": "../ng2workspace-addon-webpack",
    "ng2workspace-addon-webpack-assets": "../ng2workspace-addon-webpack-assets",
    "ng2workspace-addon-webpack-css": "../ng2workspace-addon-webpack-css",
