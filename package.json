    "ng2workspace-addon-webpack-ts": "../ng2workspace-addon-webpack-ts",
    "ng2workspace-addon-webpack-tslint": "../ng2workspace-addon-webpack-tslint",
    "typescript": "^1.7.5",
    "typings": "^0.6.8"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "patch:middleware": "npm install git://github.com/Merott/webpack-dev-middleware#309f25287b83b32d42759f1eaa76230283ac7517",
    "postinstall": "npm run patch:middleware",
    "start": "gulp webpack/webpack:watch",
    "start:prod": "gulp webpack/webpack:watch"
  }
